{
  "name": "hotel-booking-api",
  "version": "1.0.0",
  "description": "A hotel booking API built with Node.js, Express and TypeScript.",
  "main": "src/app.ts",
  "scripts": {
    "dev": "npx ts-node-dev --respawn --transpile-only src/app.ts",
    "dev:clean": "npm run clean && npm run dev",
    "build": "tsc",
    "start": "node dist/app.js",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "vercel-build": "npm run build"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.5.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.0.0",
    "nodemon": "^3.1.4",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "author": "Your Name",
  "license": "MIT"
}
